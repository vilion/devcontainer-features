{
  "name": "Neovim",
  "id": "neovim",
  "version": "0.1.7",
  "description": "Provide neovim features for devcontainers",
  "options": {
    "version": {
      "type": "string",
      "proposals": [
        "v0.9.4",
        "v0.9.5",
        "stable",
        "nightly"
      ],
      "default": "stable",
      "description": "Select the neovim version you want to install as present in GitHub Releases"
    }
  },
  "installsAfter": [
    "ghcr.io/vilion/devcontainer-features/neovim-essentials:latest"
  ]
}
