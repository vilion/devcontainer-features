{
  "name": "AstroNvim",
  "id": "astronvim",
  "version": "0.1.4",
  "description": "Provides astronvim features for devcontainers",
  "options": {
    "version": {
      "type": "string",
      "proposals": [
        "v3.43.4",
        "v3.43.3"
      ],
      "default": "v3.43.4",
      "description": "Select the astronvim version you want to install as present in GitHub Releases"
    }
  },
  "installsAfter": [
    "ghcr.io/vilion/devcontainer-features/neovim-essentials:latest",
    "ghcr.io/vilion/devcontainer-features/neovim:latest"
  ]
}
